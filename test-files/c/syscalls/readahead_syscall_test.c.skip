#include <sys/syscall.h>
#include <unistd.h>

int main() {
    int fd = syscall(SYS_memfd_create, "readahead_test", 0);
    if (fd < 0) {
        return 1;
    }
    syscall(SYS_write, fd, "abc", 3);
    long res = syscall(SYS_readahead, fd, 0, 3);
    syscall(SYS_close, fd);
    return res == 0 || res < 0 ? 0 : 1;
}
