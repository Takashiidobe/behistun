#include <sys/syscall.h>
#include <unistd.h>

struct robust_list_head {
    struct robust_list_head *next;
    long futex_offset;
    struct robust_list_head *list_op_pending;
};

int main() {
    struct robust_list_head *head = 0;
    size_t len = 0;
    long res = syscall(SYS_get_robust_list, 0, &head, &len);
    (void)res;
    return 0;
}
