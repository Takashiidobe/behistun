#include <sys/syscall.h>
#include <unistd.h>
#include <stdint.h>

struct mount_attr {
    uint64_t attr_set;
    uint64_t attr_clr;
    uint64_t propagation;
    uint64_t userns_fd;
};

int main() {
    struct mount_attr ma = {0};
    syscall(SYS_mount_setattr, AT_FDCWD, ".", 0, &ma, sizeof(ma));
    return 0;
}
