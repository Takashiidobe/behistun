#include <sys/mman.h>
#include <sys/syscall.h>
#include <unistd.h>

int main() {
    void *p = (void *)syscall(SYS_mmap, 0, 4096, PROT_READ | PROT_WRITE, MAP_PRIVATE | MAP_ANONYMOUS, -1, 0);
    if (p == MAP_FAILED) {
        return 1;
    }

    ((char *)p)[0] = 1;
    syscall(SYS_munmap, p, 4096);
    return 0;
}
